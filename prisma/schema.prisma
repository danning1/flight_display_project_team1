// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FlightInfomation{
  id String @id
  flightgNumber Int
  flightModel String
  departureTime DateTime
  departureFrom String
  expectingArrivingTime DateTime
  expectingArriveAt String
  totalRange Float
}
